{
  "name": "ulv",
  "displayName": "ulv",
  "publisher": "netr0m",
  "description": "",
  "version": "0.1.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/netr0m/vscode-ulv"
  },
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ulv.start",
        "title": "ulv: Start time entry"
      },
      {
        "command": "ulv.stop",
        "title": "ulv: Stop time entry"
      },
      {
        "command": "ulv.update",
        "title": "ulv: Force update the status bar"
      },
      {
        "command": "ulv.open",
        "title": "ulv: Open in browser"
      },
      {
        "command": "ulv.setApiKey",
        "title": "ulv: Set Toggl API key"
      }
    ],
    "configuration": {
      "title": "ulv configuration",
      "properties": {
        "ulv.apiKey": {
          "type": "string",
          "description": "The API key for your Toggl account",
          "minLength": 32,
          "maxLength": 32
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts --fix",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/luxon": "^3.2.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.76.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vscode/test-electron": "^2.2.3",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "prettier": "^2.8.5",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "got": "^11.8.6",
    "luxon": "^3.3.0"
  }
}
